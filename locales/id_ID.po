#: lua/groupbutler/api_errors.lua:19
msgid "An unknown error has ocurred"
msgstr "Sebuah kesalahan aneh telah terjadi."

#: lua/groupbutler/api_errors.lua:20
msgid "I don't have enough permissions to restrict users"
msgstr "Saya tidak memiliki kuasa untuk membatasi pengguna."

#: lua/groupbutler/api_errors.lua:21
msgid "I'm not an admin, I can't kick people"
msgstr "Saya bukan seorang admin. Saya tidak bisa menendang pengguna."

#: lua/groupbutler/api_errors.lua:22
msgid "I can't do that to admins!"
msgstr "Saya tidak dapat melakukan itu kepada para admin!"

